        app:layout_constraintEnd_toEndOf="@+id/textView238"
        app:layout_constraintStart_toStartOf="@+id/textView238"
        app:layout_constraintTop_toTopOf="@+id/textView238"
        app:layout_constraintVertical_bias="0.24000001" />

    <TextView
        android:id="@+id/textView239"
        android:layout_width="0dp"
        android:layout_height="@dimen/_1sdp"
        android:background="@color/border_color"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout42"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout42"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout42" />

</androidx.constraintlayout.widget.ConstraintLayout>