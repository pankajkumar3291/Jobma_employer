            app:layout_constraintBottom_toBottomOf="@+id/textView235"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView235"
            app:layout_constraintTop_toTopOf="@+id/textView235" />

        <ImageView
            android:id="@+id/imageView78"
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            app:layout_constraintBottom_toBottomOf="@+id/textView230"
